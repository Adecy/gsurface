   "source": [
    "from gsurface.forces import Gravity\n",
    "\n",
    "from gsurface.serialize import dumps, loads\n",
    "\n",
    "g = Gravity(1.0, [1.0, -2.0, 2.0])\n",
    "\n",
    "s = dumps(g, indent = 4)\n",
    "\n",
    "print(g)\n",
    "print(s)\n",
    "\n",
    "o = loads(s)\n",
    "\n",
    "print(o)"
   ]
  }
